  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "jsdom": "^23.0.1",
    "node-html-parser": "^7.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4",
    "wait-on": "^8.0.4"
