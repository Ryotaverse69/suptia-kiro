{
  "name": "@suptia/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "pnpm run typecheck",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:performance": "vitest run src/__tests__/performance/",
    "test:performance:watch": "vitest src/__tests__/performance/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test --grep=\"A11y Light E2E\"",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "check:dod": "node ../../scripts/check-dod.mjs"
  },
  "dependencies": {
    "@sanity/client": "^7.8.2",
    "@sanity/vision": "^4.3.0",
    "groq": "^4.3.0",
    "next": "14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.99.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@9.0.0"
}
