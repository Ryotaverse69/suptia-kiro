{
  "name": "@suptia/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "pnpm run typecheck",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run src/test/integration.test.tsx --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "playwright test src/lib/__tests__/accessibility-e2e.test.ts",
    "test:all": "node scripts/run-tests.mjs",
    "test:coverage": "vitest run --coverage",
    "test:ci": "pnpm test:unit && pnpm test:integration",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "test:fix": "vitest run --reporter=verbose",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start:production": "NODE_ENV=production next start",
    "health-check": "node scripts/health-check.mjs",
    "security-audit": "npm audit --audit-level=moderate",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "sitemap:generate": "next build && node scripts/generate-sitemap.mjs",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "headers": "cd ../.. && node scripts/check-headers.mjs",
    "jsonld": "cd ../.. && node scripts/validate-jsonld.mjs",
    "dod:check": "cd ../.. && node scripts/check-dod.mjs",
    "preview:verify": "node ../../scripts/verify-preview-env.mjs",
    "preview:url": "node ../../scripts/get-preview-url.mjs",
    "perf:optimize": "node ../../scripts/performance-optimizer.mjs",
    "perf:report": "node ../../scripts/performance-optimizer.mjs --report",
    "perf:cleanup": "node ../../scripts/performance-optimizer.mjs --cleanup"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@sanity/client": "^7.8.2",
    "@sanity/vision": "^4.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "groq": "^4.3.0",
    "next": "14.2.5",
    "next-intl": "^4.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.99.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@9.0.0"
}
